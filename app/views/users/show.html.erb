<h1><%= @user.name %>のページ</h1>
<br>
<% if @user.image? %>
  <%= image_tag @user.image.url, class: "user-icon" %>
<% else %>
  <%= image_tag "default.jpg", alt: "user-icon", size: "200", class: "user-icon" %>
<% end %>
<p>メールアドレス: <%= @user.email %></p>
<p><%= link_to 'list' , feeds_path %></p>
<p><%= link_to 'profile_image' , edit_user_path %></p>
